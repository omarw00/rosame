(define (domain rover)
(:requirements :strips :typing :negative-preconditions :equality)
(:types 	rover waypoint store camera mode lander objective - object
)

(:predicates (at ?x - rover ?y - waypoint)
	(at_lander ?x - lander ?y - waypoint)
	(can_traverse ?r - rover ?x - waypoint ?y - waypoint)
	(equipped_for_soil_analysis ?r - rover)
	(equipped_for_rock_analysis ?r - rover)
	(equipped_for_imaging ?r - rover)
	(empty ?s - store)
	(have_rock_analysis ?r - rover ?w - waypoint)
	(have_soil_analysis ?r - rover ?w - waypoint)
	(full ?s - store)
	(calibrated ?c - camera ?r - rover)
	(supports ?c - camera ?m - mode)
	(available ?r - rover)
	(visible ?w - waypoint ?p - waypoint)
	(have_image ?r - rover ?o - objective ?m - mode)
	(communicated_soil_data ?w - waypoint)
	(communicated_rock_data ?w - waypoint)
	(communicated_image_data ?o - objective ?m - mode)
	(at_soil_sample ?w - waypoint)
	(at_rock_sample ?w - waypoint)
	(visible_from ?o - objective ?w - waypoint)
	(store_of ?s - store ?r - rover)
	(calibration_target ?i - camera ?o - objective)
	(on_board ?i - camera ?r - rover)
	(channel_free ?l - lander)
)

(:action navigate
	:parameters (?x - rover ?y - waypoint ?z - waypoint)
	:precondition (and (at ?x ?y)
	(at ?x ?z)
	(at_rock_sample ?y)
	(at_rock_sample ?z)
	(at_soil_sample ?y)
	(at_soil_sample ?z)
	(available ?x)
	(can_traverse ?x ?y ?z)
	(can_traverse ?x ?z ?y)
	(communicated_rock_data ?y)
	(communicated_rock_data ?z)
	(communicated_soil_data ?y)
	(communicated_soil_data ?z)
	(equipped_for_imaging ?x)
	(have_rock_analysis ?x ?y)
	(have_rock_analysis ?x ?z)
	(have_soil_analysis ?x ?y)
	(have_soil_analysis ?x ?z)
	(visible ?y ?z)
	(visible ?z ?y)(not (= ?y ?z)))
	:effect (and (at ?x ?y)
		(at ?x ?z)
		(at_rock_sample ?y)
		(at_rock_sample ?z)
		(at_soil_sample ?y)
		(at_soil_sample ?z)
		(can_traverse ?x ?y ?z)
		(can_traverse ?x ?z ?y)
		(communicated_rock_data ?y)
		(communicated_rock_data ?z)
		(communicated_soil_data ?y)
		(communicated_soil_data ?z)
		(equipped_for_imaging ?x)
		(have_rock_analysis ?x ?y)
		(have_rock_analysis ?x ?z)
		(have_soil_analysis ?x ?y)
		(have_soil_analysis ?x ?z)
		(not (at ?x ?y))
		(not (at ?x ?z))
		(not (at_rock_sample ?y))
		(not (at_rock_sample ?z))
		(not (at_soil_sample ?y))
		(not (at_soil_sample ?z))
		(not (can_traverse ?x ?y ?z))
		(not (can_traverse ?x ?z ?y))
		(not (communicated_rock_data ?y))
		(not (communicated_rock_data ?z))
		(not (communicated_soil_data ?y))
		(not (communicated_soil_data ?z))
		(not (equipped_for_imaging ?x))
		(not (have_rock_analysis ?x ?y))
		(not (have_rock_analysis ?x ?z))
		(not (have_soil_analysis ?x ?y))
		(not (have_soil_analysis ?x ?z))
		(not (visible ?y ?z))
		(not (visible ?z ?y))
		(visible ?y ?z)
		(visible ?z ?y) 
		))

(:action sample_soil
	:parameters (?x - rover ?s - store ?p - waypoint)
	:precondition (and (at ?x ?p)
	(at_rock_sample ?p)
	(at_soil_sample ?p)
	(available ?x)
	(communicated_rock_data ?p)
	(communicated_soil_data ?p)
	(empty ?s)
	(equipped_for_imaging ?x)
	(full ?s)
	(have_rock_analysis ?x ?p)
	(have_soil_analysis ?x ?p))
	:effect (and (at ?x ?p)
		(at_rock_sample ?p)
		(at_soil_sample ?p)
		(communicated_rock_data ?p)
		(communicated_soil_data ?p)
		(empty ?s)
		(equipped_for_imaging ?x)
		(full ?s)
		(have_rock_analysis ?x ?p)
		(have_soil_analysis ?x ?p)
		(not (at ?x ?p))
		(not (at_rock_sample ?p))
		(not (at_soil_sample ?p))
		(not (communicated_rock_data ?p))
		(not (communicated_soil_data ?p))
		(not (empty ?s))
		(not (equipped_for_imaging ?x))
		(not (full ?s))
		(not (have_rock_analysis ?x ?p))
		(not (have_soil_analysis ?x ?p)) 
		))

(:action sample_rock
	:parameters (?x - rover ?s - store ?p - waypoint)
	:precondition (and (at ?x ?p)
	(at_rock_sample ?p)
	(at_soil_sample ?p)
	(available ?x)
	(communicated_rock_data ?p)
	(communicated_soil_data ?p)
	(empty ?s)
	(equipped_for_imaging ?x)
	(full ?s)
	(have_rock_analysis ?x ?p)
	(have_soil_analysis ?x ?p))
	:effect (and (at ?x ?p)
		(at_rock_sample ?p)
		(at_soil_sample ?p)
		(communicated_rock_data ?p)
		(communicated_soil_data ?p)
		(equipped_for_imaging ?x)
		(full ?s)
		(have_rock_analysis ?x ?p)
		(have_soil_analysis ?x ?p)
		(not (at ?x ?p))
		(not (at_rock_sample ?p))
		(not (at_soil_sample ?p))
		(not (communicated_rock_data ?p))
		(not (communicated_soil_data ?p))
		(not (empty ?s))
		(not (equipped_for_imaging ?x))
		(not (full ?s))
		(not (have_rock_analysis ?x ?p))
		(not (have_soil_analysis ?x ?p)) 
		))

(:action drop
	:parameters (?x - rover ?y - store)
	:precondition (and (available ?x)
	(equipped_for_imaging ?x)
	(full ?y))
	:effect (and (empty ?y)
		(equipped_for_imaging ?x)
		(full ?y)
		(not (equipped_for_imaging ?x))
		(not (full ?y)) 
		))

(:action calibrate
	:parameters (?i - camera ?t - objective ?r - rover ?w - waypoint)
	:precondition (and (at ?r ?w)
	(at_rock_sample ?w)
	(at_soil_sample ?w)
	(available ?r)
	(calibration_target ?i ?t)
	(communicated_rock_data ?w)
	(communicated_soil_data ?w)
	(equipped_for_imaging ?r)
	(have_rock_analysis ?r ?w)
	(have_soil_analysis ?r ?w)
	(visible_from ?t ?w))
	:effect (and (at ?r ?w)
		(at_rock_sample ?w)
		(at_soil_sample ?w)
		(calibrated ?i ?r)
		(calibration_target ?i ?t)
		(communicated_rock_data ?w)
		(communicated_soil_data ?w)
		(equipped_for_imaging ?r)
		(have_rock_analysis ?r ?w)
		(have_soil_analysis ?r ?w)
		(not (at ?r ?w))
		(not (at_rock_sample ?w))
		(not (at_soil_sample ?w))
		(not (calibration_target ?i ?t))
		(not (communicated_rock_data ?w))
		(not (communicated_soil_data ?w))
		(not (equipped_for_imaging ?r))
		(not (have_rock_analysis ?r ?w))
		(not (have_soil_analysis ?r ?w))
		(not (visible_from ?t ?w))
		(visible_from ?t ?w) 
		))

(:action take_image
	:parameters (?i - camera ?m - mode ?o - objective ?r - rover ?p - waypoint)
	:precondition (and (at ?r ?p)
	(at_rock_sample ?p)
	(at_soil_sample ?p)
	(available ?r)
	(calibrated ?i ?r)
	(calibration_target ?i ?o)
	(communicated_rock_data ?p)
	(communicated_soil_data ?p)
	(equipped_for_imaging ?r)
	(have_rock_analysis ?r ?p)
	(have_soil_analysis ?r ?p)
	(visible_from ?o ?p))
	:effect (and (at ?r ?p)
		(at_rock_sample ?p)
		(at_soil_sample ?p)
		(calibration_target ?i ?o)
		(communicated_rock_data ?p)
		(communicated_soil_data ?p)
		(equipped_for_imaging ?r)
		(have_rock_analysis ?r ?p)
		(have_soil_analysis ?r ?p)
		(not (at ?r ?p))
		(not (at_rock_sample ?p))
		(not (at_soil_sample ?p))
		(not (calibrated ?i ?r))
		(not (calibration_target ?i ?o))
		(not (communicated_rock_data ?p))
		(not (communicated_soil_data ?p))
		(not (equipped_for_imaging ?r))
		(not (have_rock_analysis ?r ?p))
		(not (have_soil_analysis ?r ?p))
		(not (visible_from ?o ?p))
		(visible_from ?o ?p) 
		))

(:action communicate_soil_data
	:parameters (?l - lander ?r - rover ?p - waypoint ?x - waypoint ?y - waypoint)
	:precondition (and (not (= ?p ?x))
	(not (= ?x ?y))
	(not (= ?p ?y)))
	:effect (and  
		))

(:action communicate_rock_data
	:parameters (?l - lander ?r - rover ?p - waypoint ?x - waypoint ?y - waypoint)
	:precondition (and (not (= ?p ?x))
	(not (= ?x ?y))
	(not (= ?p ?y)))
	:effect (and  
		))

(:action communicate_image_data
	:parameters (?l - lander ?m - mode ?o - objective ?r - rover ?x - waypoint ?y - waypoint)
	:precondition (and (not (= ?x ?y)))
	:effect (and  
		))

)