(define (domain driverlog)
(:requirements :typing :fluents :negative-preconditions :equality)
(:types 	location locatable - object
	driver truck obj - locatable
)

(:predicates (at ?obj - locatable ?loc - location)
	(in ?obj1 - obj ?obj - truck)
	(driving ?d - driver ?v - truck)
	(link ?x - location ?y - location)
	(path ?x - location ?y - location)
	(empty ?v - truck)
)

(:functions (time-to-walk ?l1 - location ?l2 - location)
	(time-to-drive ?l1 - location ?l2 - location)
	(driven )
	(walked )
)

(:action load-truck
	:parameters (?a - obj ?b - truck ?c - location)
	:precondition (and (at ?a ?c)
	(at ?b ?c)
	(<= (* (driven ) -1) 0)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.9843) (* (walked ) 0.1767)) 551.1919)
	(<= (+ (* (walked ) 0.5830) (* (driven ) -0.8124)) 83.9585)
	(<= (+ (* (walked ) 0.9630) (* (driven ) -0.2695)) 204.0918))
	:effect (and (in ?a ?b)
		(not (at ?a ?c)) 
		))

(:action unload-truck
	:parameters (?a - obj ?b - truck ?c - location)
	:precondition (and (at ?b ?c)
	(in ?a ?b)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.8878) (* (walked ) 0.4603)) 672.0302)
	(<= (+ (* (walked ) -0.7359) (* (driven ) -0.6771)) -81.2468)
	(<= (+ (* (walked ) 0.5361) (* (driven ) -0.8442)) 32.4358)
	(<= (+ (* (walked ) 0.9425) (* (driven ) -0.3343)) 180.0637)
	(<= (walked ) 351))
	:effect (and (at ?a ?c)
		(not (in ?a ?b)) 
		))

(:action board-truck
	:parameters (?a - driver ?b - truck ?c - location)
	:precondition (and (at ?a ?c)
	(at ?b ?c)
	(empty ?b)
	(<= (* (driven ) -1) 0)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.8514) (* (walked ) -0.5246)) 702.3744)
