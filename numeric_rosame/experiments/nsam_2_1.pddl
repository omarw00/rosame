(define (domain driverlog)
(:requirements :typing :fluents :negative-preconditions :equality)
(:types 	location locatable - object
	driver truck obj - locatable
)

(:predicates (at ?obj - locatable ?loc - location)
	(in ?obj1 - obj ?obj - truck)
	(driving ?d - driver ?v - truck)
	(link ?x - location ?y - location)
	(path ?x - location ?y - location)
	(empty ?v - truck)
)

(:functions (time-to-walk ?l1 - location ?l2 - location)
	(time-to-drive ?l1 - location ?l2 - location)
	(driven )
	(walked )
)

(:action load-truck
	:parameters (?obj - obj ?truck - truck ?loc - location)
	:precondition (and (at ?obj ?loc)
	(at ?truck ?loc)
	(not (in ?obj ?truck))
	(<= (* (driven ) -1) 0)
	(<= (driven ) 85)
	(= (walked ) 0))
	:effect (and (in ?obj ?truck)
		(not (at ?obj ?loc)) 
		))

(:action unload-truck
	:parameters (?obj - obj ?truck - truck ?loc - location)
	:precondition (and (at ?truck ?loc)
	(in ?obj ?truck)
	(not (at ?obj ?loc))
	(not (empty ?truck))
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.3846) (* (walked ) -0.9231)) 65.3846)
	(<= (+ (* (walked ) 0.9831) (* (driven ) -0.1829)) -9.3283))
	:effect (and (at ?obj ?loc)
		(not (in ?obj ?truck)) 
		))

(:action board-truck
	:parameters (?driver - driver ?truck - truck ?loc - location)
	:precondition (and (at ?driver ?loc)
	(at ?truck ?loc)
	(empty ?truck)
	(not (driving ?driver ?truck))
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.2880) (* (walked ) -0.9576)) 24.4807)
	(<= (+ (* (driven ) 0.8824) (* (walked ) -0.4706)) 173.5294)
	(<= (+ (* (walked ) 0.8984) (* (driven ) -0.4391)) 0))
	:effect (and (driving ?driver ?truck)
		(not (at ?driver ?loc))
		(not (empty ?truck)) 
		))

(:action disembark-truck
	:parameters (?driver - driver ?truck - truck ?loc - location)
	:precondition (and (at ?truck ?loc)
	(driving ?driver ?truck)
	(not (at ?driver ?loc))
	(not (empty ?truck))
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.9106) (* (walked ) -0.4133)) 198.5122)
	(<= (+ (* (walked ) 0.8307) (* (driven ) -0.5567)) -46.2040))
	:effect (and (at ?driver ?loc)
		(empty ?truck)
		(not (driving ?driver ?truck)) 
		))

(:action drive-truck
	:parameters (?truck - truck ?loc-from - location ?loc-to - location ?driver - driver)
	:precondition (and (at ?truck ?loc-from)
	(driving ?driver ?truck)
	(link ?loc-from ?loc-to)
	(link ?loc-to ?loc-from)
	(not (at ?driver ?loc-from))
	(not (at ?driver ?loc-to))
	(not (at ?truck ?loc-to))
	(not (empty ?truck))
	(not (path ?loc-from ?loc-to))
	(not (path ?loc-to ?loc-from))
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.3192) (+ (* (walked ) -0.4700) (* (time-to-drive ?loc-to ?loc-from) -0.8229))) 14.7702)
	(<= (+ (* (driven ) 0.3600) (+ (* (time-to-drive ?loc-to ?loc-from) 0.8269) (* (walked ) -0.4320))) 100.8873)
	(<= (+ (* (driven ) 0.4441) (+ (* (time-to-drive ?loc-to ?loc-from) -0.7202) (* (walked ) -0.5330))) 40.9320)
	(<= (+ (* (time-to-drive ?loc-to ?loc-from) 0.8691) (* (walked ) 0.4947)) 73.8705)
	(<= (+ (* (time-to-drive ?loc-to ?loc-from) 0.9400) (+ (* (driven ) 0.1730) (* (walked ) 0.2941))) 94.6046)
	(<= (+ (* (walked ) 0.0614) (* (time-to-drive ?loc-to ?loc-from) -0.9981)) -2.9943)
	(<= (+ (* (walked ) 0.7624) (+ (* (driven ) -0.4435) (* (time-to-drive ?loc-to ?loc-from) -0.4712))) -24.0308)
	(<= (+ (* (walked ) 0.8984) (* (driven ) -0.4391)) 0)
	(= (time-to-drive ?loc-from ?loc-to) (time-to-drive ?loc-to ?loc-from)))
	:effect (and (at ?truck ?loc-to)
		(not (at ?truck ?loc-from))
(increase (driven ) (time-to-drive ?loc-from ?loc-to))))

(:action walk
	:parameters (?driver - driver ?loc-from - location ?loc-to - location)
	:precondition (and (at ?driver ?loc-from)
	(not (at ?driver ?loc-to))
	(not (link ?loc-from ?loc-to))
	(not (link ?loc-to ?loc-from))
	(path ?loc-from ?loc-to)
	(path ?loc-to ?loc-from)
	(<= (* (walked ) -1) 0)
	(<= (walked ) 40)
	(= (+ (* (driven ) 1) (* (walked ) -1.2000)) 218)
	(= (+ (* (walked ) 0.3123) (+ (* (driven ) 0.3748) (+ (* (time-to-walk ?loc-to ?loc-from) 0.3904) (* (time-to-walk ?loc-from ?loc-to) -1)))) 57.3142)
	(= (+ (* (walked ) 0.5123) (+ (* (driven ) 0.6148) (* (time-to-walk ?loc-to ?loc-from) -1))) 94.0164))
	:effect (and (at ?driver ?loc-to)
		(not (at ?driver ?loc-from))
(increase (walked ) (+ (* (time-to-walk ?loc-from ?loc-to) 0.7027) (+ (* (driven ) 0.6746) (+ (* (walked ) -0.4379) -135.1655))))))

)