(:action lift
	:parameters (?x - hoist ?y - crate ?z - surface ?p - place)
	:precondition (and (at ?x ?p)
	(at ?y ?p)
	(at ?z ?p)
	(available ?x)
	(clear ?y)
	(not (clear ?z))
	(not (lifting ?x ?y))
	(on ?y ?z)
	(<= (* (fuel-cost ) -1) 0)
	(<= (+ (* (fuel-cost ) 0.3417) (* (weight ?y) -0.9398)) 7.2620)
	(<= (+ (* (weight ?y) -0.1644) (* (fuel-cost ) -0.9864)) -5.9184)
	(<= (+ (* (weight ?y) -0.6214) (* (fuel-cost ) -0.7835)) -19.4253)
	(<= (+ (* (weight ?y) 0.0948) (* (fuel-cost ) -0.9955)) 5.2145)
	(<= (+ (* (weight ?y) 0.2897) (* (fuel-cost ) 0.9571)) 46.6349)
	(<= (+ (* (weight ?y) 0.3881) (* (fuel-cost ) -0.9216)) 27.6491)
	(<= (+ (* (weight ?y) 0.7926) (* (fuel-cost ) 0.6097)) 81.3964))
	:effect (and (clear ?z)
		(lifting ?x ?y)
		(not (at ?y ?p))
		(not (available ?x))
		(not (clear ?y))
		(not (on ?y ?z))
(increase (fuel-cost ) 1)))

(:action drop
	:parameters (?x - hoist ?y - crate ?z - surface ?p - place)
	:precondition (and (at ?x ?p)
	(at ?z ?p)
	(clear ?z)
	(lifting ?x ?y)
	(not (at ?y ?p))
	(not (available ?x))
	(not (clear ?y))
	(not (on ?y ?z))
	(<= (+ (* (fuel-cost ) -0.2822) (* (weight ?y) -0.9594)) -27.5394)
	(<= (+ (* (fuel-cost ) 0.9377) (* (weight ?y) -0.3473)) 59.7033)
	(<= (+ (* (weight ?y) -0.1390) (* (fuel-cost ) -0.9903)) -35.3205)
