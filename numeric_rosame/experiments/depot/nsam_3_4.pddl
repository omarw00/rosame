	(<= (+ (* (weight ?y) 0.9615) (* (fuel-cost ) 0.2747)) 102.8831)
	(<= (fuel-cost ) 77))
	:effect (and (at ?y ?p)
		(available ?x)
		(clear ?y)
		(not (clear ?z))
		(not (lifting ?x ?y))
		(on ?y ?z) 
		))

