	(<= (+ (* (load_limit ?z) 0.3484) (+ (* (weight ?y) -0.9075) (+ (* (current_load ?z) -0.0425) (* (fuel-cost ) -0.2306)))) 63.6055)
	(<= (+ (* (load_limit ?z) 0.3897) (+ (* (weight ?y) -0.9017) (+ (* (fuel-cost ) -0.1610) (* (current_load ?z) -0.0954)))) 79.6115)
	(<= (+ (* (load_limit ?z) 0.4011) (+ (* (fuel-cost ) 0.3627) (+ (* (current_load ?z) -0.2944) (* (weight ?y) -0.7879)))) 121.4255)
	(<= (+ (* (weight ?y) 0.2051) (+ (* (load_limit ?z) -0.6036) (+ (* (current_load ?z) -0.0429) (* (fuel-cost ) -0.7693)))) -212.2932)
	(<= (+ (* (weight ?y) 0.9621) (+ (* (current_load ?z) 0.1761) (+ (* (fuel-cost ) 0.1924) (* (load_limit ?z) -0.0802)))) 81.8065)
	(<= (load_limit ?z) 374))
	:effect (and (available ?x)
		(in ?y ?z)
		(not (lifting ?x ?y))
(increase (current_load ?z) (weight ?y))))

