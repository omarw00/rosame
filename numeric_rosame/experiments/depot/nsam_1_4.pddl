	(<= (+ (* (current_load ?z) 0.3491) (+ (* (weight ?y) 0.7855) (+ (* (fuel-cost ) 0.4744) (* (load_limit ?z) -0.1901)))) 68.8571)
	(<= (+ (* (current_load ?z) 0.3966) (* (load_limit ?z) -0.9180)) -244.1884)
	(<= (+ (* (current_load ?z) 0.4566) (+ (* (fuel-cost ) 0.3586) (+ (* (load_limit ?z) -0.7887) (* (weight ?y) -0.2020)))) -187.1640)
	(<= (+ (* (fuel-cost ) 0.8547) (+ (* (load_limit ?z) -0.0479) (+ (* (current_load ?z) -0.1881) (* (weight ?y) -0.4815)))) 41.1921)
	(<= (+ (* (fuel-cost ) 0.9803) (+ (* (load_limit ?z) -0.0801) (+ (* (current_load ?z) -0.0733) (* (weight ?y) -0.1649)))) 73.4958)
