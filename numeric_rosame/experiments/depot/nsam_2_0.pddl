	(<= (+ (* (weight ?y) 0.9522) (* (fuel-cost ) -0.3054)) 65.7388))
	:effect (and (at ?y ?p)
		(available ?x)
		(clear ?y)
		(not (clear ?z))
		(not (lifting ?x ?y))
		(on ?y ?z) 
		))

