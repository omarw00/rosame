(define (domain depot)
(:requirements :typing :fluents :negative-preconditions :equality)
(:types 	place locatable - object
	depot distributor - place
	truck hoist surface - locatable
	pallet crate - surface
)

(:predicates (at ?x - locatable ?y - place)
	(on ?x - crate ?y - surface)
	(in ?x - crate ?y - truck)
	(lifting ?x - hoist ?y - crate)
	(available ?x - hoist)
	(clear ?x - surface)
)

(:functions (load_limit ?t - truck)
	(current_load ?t - truck)
	(weight ?c - crate)
	(fuel-cost )
)

(:action drive
	:parameters (?a - truck ?b - place ?c - place)
	:precondition (and (at ?a ?b)
	(<= (+ (* (current_load ?a) -0.9580) (* (load_limit ?a) -0.2867)) -101.4940)
	(<= (+ (* (current_load ?a) 0.9191) (+ (* (load_limit ?a) -0.0789) (* (fuel-cost ) -0.3860))) 18.6329)
	(<= (+ (* (current_load ?a) 0.9625) (+ (* (load_limit ?a) -0.1972) (* (fuel-cost ) -0.1863))) -4.2546)
	(<= (+ (* (fuel-cost ) 0.1784) (+ (* (load_limit ?a) -0.3437) (* (current_load ?a) -0.9220))) -111.4823)
	(<= (+ (* (fuel-cost ) 0.9720) (+ (* (load_limit ?a) -0.0197) (* (current_load ?a) -0.2342))) 48.4435)
	(<= (+ (* (load_limit ?a) 0.1302) (+ (* (current_load ?a) 0.1732) (* (fuel-cost ) -0.9762))) 30.4777)
	(<= (load_limit ?a) 354))
	:effect (and (at ?a ?c)
		(not (at ?a ?b))
(increase (fuel-cost ) 10)))

(:action lift
	:parameters (?a - hoist ?b - crate ?c - surface ?d - place)
	:precondition (and (at ?a ?d)
	(at ?b ?d)
	(at ?c ?d)
	(available ?a)
	(clear ?b)
	(on ?b ?c)
	(<= (+ (* (fuel-cost ) 0.6743) (* (weight ?b) 0.7385)) 38.9471)
	(<= (+ (* (fuel-cost ) 0.8396) (* (weight ?b) -0.5433)) 6.0745)
	(<= (+ (* (weight ?b) -0.6616) (* (fuel-cost ) -0.7498)) -19.1870)
	(<= (+ (* (weight ?b) 0.1047) (* (fuel-cost ) -0.9945)) 3.0359)
	(<= (+ (* (weight ?b) 0.4472) (* (fuel-cost ) -0.8944)) 19.6774))
	:effect (and (clear ?c)
		(lifting ?a ?b)
		(not (at ?b ?d))
		(not (available ?a))
		(not (clear ?b))
		(not (on ?b ?c))
(increase (fuel-cost ) 1)))

(:action drop
	:parameters (?a - hoist ?b - crate ?c - surface ?d - place)
	:precondition (and (at ?a ?d)
	(at ?c ?d)
	(clear ?c)
	(lifting ?a ?b)
	(<= (+ (* (fuel-cost ) 0.9029) (* (weight ?b) -0.4299)) 64.3611)
	(<= (+ (* (weight ?b) -0.8969) (* (fuel-cost ) -0.4423)) -44.8190)
	(<= (+ (* (weight ?b) 0.9997) (* (fuel-cost ) -0.0241)) 47.8897)
	(<= (fuel-cost ) 87))
	:effect (and (at ?b ?d)
		(available ?a)
		(clear ?b)
		(not (clear ?c))
		(not (lifting ?a ?b))
		(on ?b ?c) 
		))

(:action load
	:parameters (?a - hoist ?b - crate ?c - truck ?d - place)
	:precondition (and (at ?a ?d)
	(at ?c ?d)
	(lifting ?a ?b)
	(<= (* (current_load ?c) -1) 0)
	(<= (+ (* (current_load ?c) 0.1585) (+ (* (load_limit ?c) -0.0524) (+ (* (weight ?b) -0.8931) (* (fuel-cost ) -0.4177)))) -50.0688)
	(<= (+ (* (current_load ?c) 0.3217) (+ (* (weight ?b) 0.9462) (+ (* (fuel-cost ) 0.0019) (* (load_limit ?c) -0.0341)))) 35.3785)
	(<= (+ (* (current_load ?c) 0.8435) (+ (* (fuel-cost ) -0.1633) (+ (* (weight ?b) -0.3490) (* (load_limit ?c) -0.3742)))) -96.3227)
	(<= (+ (* (fuel-cost ) 0.1749) (+ (* (weight ?b) 0.3738) (+ (* (current_load ?c) -0.9034) (* (load_limit ?c) -0.1164)))) -9.0688)
	(<= (+ (* (fuel-cost ) 0.2891) (+ (* (current_load ?c) 0.8469) (+ (* (load_limit ?c) -0.2765) (* (weight ?b) -0.3504)))) -44.8278)
	(<= (+ (* (fuel-cost ) 0.9973) (* (load_limit ?c) -0.0728)) 51.0248)
	(<= (+ (* (load_limit ?c) 0.0120) (+ (* (current_load ?c) -0.1091) (+ (* (fuel-cost ) -0.4211) (* (weight ?b) -0.9003)))) -36.4202)
	(<= (+ (* (load_limit ?c) 0.1565) (+ (* (fuel-cost ) 0.0883) (+ (* (current_load ?c) -0.0335) (* (weight ?b) -0.9832)))) 28.0724)
	(<= (+ (* (load_limit ?c) 0.1830) (+ (* (fuel-cost ) 0.1152) (+ (* (current_load ?c) -0.1175) (* (weight ?b) -0.9692)))) 35.8058)
	(<= (+ (* (load_limit ?c) 0.2227) (+ (* (fuel-cost ) 0.2934) (+ (* (current_load ?c) -0.2993) (* (weight ?b) -0.8802)))) 57.4270)
	(<= (+ (* (weight ?b) 0.9375) (+ (* (load_limit ?c) 0.1026) (+ (* (current_load ?c) 0.1136) (* (fuel-cost ) -0.3125)))) 59.1488)
	(<= (+ (* (weight ?b) 0.9379) (+ (* (load_limit ?c) 0.1014) (+ (* (current_load ?c) 0.1176) (* (fuel-cost ) -0.3101)))) 58.8992)
	(<= (+ (* (weight ?b) 0.9555) (+ (* (current_load ?c) 0.2877) (+ (* (load_limit ?c) -0.0335) (* (fuel-cost ) -0.0557)))) 31.6269)
	(<= (load_limit ?c) 354))
	:effect (and (available ?a)
		(in ?b ?c)
		(not (lifting ?a ?b))
(increase (current_load ?c) (weight ?b))))

(:action unload
	:parameters (?a - hoist ?b - crate ?c - truck ?d - place)
	:precondition (and (at ?a ?d)
	(at ?c ?d)
	(available ?a)
	(in ?b ?c)
	(<= (+ (* (current_load ?c) 0.8830) (+ (* (weight ?b) 0.3654) (* (load_limit ?c) -0.2947))) -12.7669)
	(<= (+ (* (fuel-cost ) 0.1253) (+ (* (load_limit ?c) 0.1470) (+ (* (current_load ?c) 0.0404) (* (weight ?b) -0.9803)))) 31.9324)
	(<= (+ (* (fuel-cost ) 0.1709) (+ (* (weight ?b) -0.3752) (+ (* (current_load ?c) -0.9068) (* (load_limit ?c) -0.0873)))) -58.3558)
	(<= (+ (* (fuel-cost ) 0.9973) (* (load_limit ?c) -0.0728)) 60.9983)
	(<= (+ (* (load_limit ?c) 0.0304) (+ (* (current_load ?c) -0.1305) (+ (* (fuel-cost ) -0.4044) (* (weight ?b) -0.9047)))) -42.3236)
	(<= (+ (* (load_limit ?c) 0.0365) (+ (* (weight ?b) 0.9461) (* (current_load ?c) -0.3217))) 33.5154)
	(<= (+ (* (load_limit ?c) 0.0791) (+ (* (weight ?b) 0.9405) (+ (* (current_load ?c) -0.3198) (* (fuel-cost ) -0.0828)))) 41.3009)
	(<= (+ (* (load_limit ?c) 0.1412) (+ (* (weight ?b) 0.9123) (+ (* (fuel-cost ) -0.3842) (* (current_load ?c) -0.0067)))) 61.6345)
	(<= (+ (* (load_limit ?c) 0.1438) (+ (* (current_load ?c) 0.2906) (+ (* (fuel-cost ) 0.4055) (* (weight ?b) -0.8547)))) 67.5777)
	(<= (+ (* (load_limit ?c) 0.1498) (+ (* (weight ?b) 0.9245) (+ (* (fuel-cost ) -0.2882) (* (current_load ?c) -0.1994)))) 57.6184)
	(<= (+ (* (load_limit ?c) 0.2591) (+ (* (weight ?b) 0.1465) (+ (* (current_load ?c) -0.2931) (* (fuel-cost ) -0.9086)))) 35.6733)
	(<= (+ (* (weight ?b) -0.3799) (+ (* (load_limit ?c) -0.1137) (* (current_load ?c) -0.9180))) -77.8834)
	(<= (+ (* (weight ?b) 0.3539) (+ (* (current_load ?c) 0.8884) (+ (* (load_limit ?c) -0.2922) (* (fuel-cost ) -0.0065)))) -12.5839)
	(<= (+ (* (weight ?b) 0.7071) (* (current_load ?c) -0.7071)) 0)
	(<= (load_limit ?c) 354))
	:effect (and (lifting ?a ?b)
		(not (available ?a))
		(not (in ?b ?c))
(decrease (current_load ?c) (weight ?b))))

)