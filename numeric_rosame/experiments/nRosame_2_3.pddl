(define (domain driverlog)
(:requirements :typing :fluents :negative-preconditions :equality)
(:types 	location locatable - object
	driver truck obj - locatable
)

(:predicates (at ?obj - locatable ?loc - location)
	(in ?obj1 - obj ?obj - truck)
	(driving ?d - driver ?v - truck)
	(link ?x - location ?y - location)
	(path ?x - location ?y - location)
	(empty ?v - truck)
)

(:functions (time-to-walk ?l1 - location ?l2 - location)
	(time-to-drive ?l1 - location ?l2 - location)
	(driven )
	(walked )
)

(:action load-truck
	:parameters (?a - obj ?b - truck ?c - location)
	:precondition (and (at ?a ?c)
	(at ?b ?c)
	(<= (* (walked ) -1) -53)
	(<= (+ (* (driven ) -0.2486) (* (walked ) -0.9686)) -93.8438)
	(<= (+ (* (walked ) 0.5865) (* (driven ) 0.8099)) 233.5706)
	(<= (walked ) 82))
	:effect (and (in ?a ?b)
		(not (at ?a ?c)) 
		))

(:action unload-truck
	:parameters (?a - obj ?b - truck ?c - location)
	:precondition (and (at ?b ?c)
	(in ?a ?b)
	(<= (* (walked ) -1) -53)
	(<= (+ (* (driven ) -0.1644) (* (walked ) -0.9864)) -99.9546)
	(<= (+ (* (walked ) 0.5579) (* (driven ) 0.8299)) 379.7865)
	(<= (+ (* (walked ) 0.9290) (* (driven ) -0.3700)) 33.2647))
	:effect (and (at ?a ?c)
		(not (in ?a ?b)) 
		))

(:action board-truck
	:parameters (?a - driver ?b - truck ?c - location)
	:precondition (and (at ?a ?c)
	(at ?b ?c)
	(empty ?b)
	(<= (+ (* (driven ) 0.1246) (* (walked ) -0.9922)) 0)
	(<= (+ (* (driven ) 0.6530) (* (walked ) 0.7573)) 315.7241)
	(<= (+ (* (walked ) 0.7797) (* (driven ) -0.6261)) 0)
	(<= (+ (* (walked ) 0.8777) (* (driven ) -0.4791)) 14.8975))
	:effect (and (driving ?a ?b)
		(not (at ?a ?c))
		(not (empty ?b)) 
		))

(:action disembark-truck
	:parameters (?a - driver ?b - truck ?c - location)
	:precondition (and (at ?b ?c)
	(driving ?a ?b)
	(<= (* (walked ) -1) -53)
	(<= (+ (* (driven ) 0.6318) (* (walked ) 0.7751)) 349.3964)
	(<= (+ (* (walked ) 0.9183) (* (driven ) -0.3959)) 22.5378))
	:effect (and (at ?a ?c)
		(empty ?b)
		(not (driving ?a ?b)) 
		))

(:action drive-truck
	:parameters (?a - truck ?b - location ?c - location ?d - driver)
	:precondition (and (at ?a ?b)
	(driving ?d ?a)
	(link ?b ?c)
	(link ?c ?b)
	(<= (+ (* (driven ) 0.0219) (+ (* (time-to-drive ?c ?b) -0.8529) (* (walked ) -0.5216))) -56.2930)
	(<= (+ (* (driven ) 0.0254) (+ (* (walked ) 0.1424) (* (time-to-drive ?c ?b) -0.9895))) -25.6881)
	(<= (+ (* (driven ) 0.0962) (+ (* (time-to-drive ?c ?b) -0.6361) (* (walked ) -0.7656))) -41.9821)
	(<= (+ (* (time-to-drive ?c ?b) -0.8910) (* (walked ) -0.4539)) -58.8086)
	(<= (+ (* (time-to-drive ?c ?b) 0.9862) (+ (* (walked ) 0.1581) (* (driven ) -0.0501))) 70.1576)
	(<= (+ (* (time-to-drive ?c ?b) 0.9977) (* (walked ) 0.0671)) 69.4064)
	(<= (+ (* (walked ) -0.0958) (+ (* (time-to-drive ?c ?b) -0.9821) (* (driven ) -0.1624))) -64.8160)
	(<= (+ (* (walked ) 0.0968) (+ (* (time-to-drive ?c ?b) 0.9923) (* (driven ) -0.0777))) 65.4893)
	(<= (+ (* (walked ) 0.1063) (+ (* (driven ) 0.1486) (* (time-to-drive ?c ?b) -0.9832))) 3.4593)
	(<= (+ (* (walked ) 0.1577) (* (time-to-drive ?c ?b) -0.9875)) -30.1559)
	(<= (+ (* (walked ) 0.2582) (+ (* (driven ) -0.0818) (* (time-to-drive ?c ?b) -0.9626))) -34.6580)
	(<= (time-to-drive ?c ?b) 66)
	(= (time-to-drive ?b ?c) (time-to-drive ?c ?b)))
	:effect (and (at ?a ?c)
		(not (at ?a ?b))
(increase (driven ) (time-to-drive ?b ?c))))

(:action walk
	:parameters (?a - driver ?b - location ?c - location)
	:precondition (and (at ?a ?b)
	(path ?b ?c)
	(path ?c ?b)
	(<= (+ (* (driven ) 0.0247) (+ (* (time-to-walk ?c ?b) 0.9915) (* (walked ) -0.1278))) 52.1992)
	(<= (+ (* (driven ) 0.1855) (+ (* (walked ) -0.6046) (* (time-to-walk ?c ?b) -0.7747))) -20.1412)
	(<= (+ (* (driven ) 0.2289) (+ (* (time-to-walk ?c ?b) -0.6042) (* (walked ) -0.7632))) -15.7102)
	(<= (+ (* (driven ) 0.3169) (+ (* (time-to-walk ?c ?b) 0.6707) (* (walked ) -0.6707))) 55.1178)
	(<= (+ (* (time-to-walk ?c ?b) 0.0870) (+ (* (walked ) 0.9531) (* (driven ) -0.2898))) 29.6530)
	(<= (+ (* (time-to-walk ?c ?b) 0.6357) (+ (* (walked ) -0.7335) (* (driven ) -0.2408))) 16.5272)
	(<= (+ (* (walked ) 0.5837) (+ (* (time-to-walk ?c ?b) -0.5059) (* (driven ) -0.6350))) -13.1538)
	(<= (+ (* (walked ) 0.7071) (+ (* (driven ) -0.0097) (* (time-to-walk ?c ?b) -0.7071))) 34.0044)
	(= (time-to-walk ?b ?c) (time-to-walk ?c ?b)))
	:effect (and (at ?a ?c)
		(not (at ?a ?b))
(increase (walked ) (time-to-walk ?b ?c))))

)