(define (domain driverlog)
(:requirements :typing :fluents :negative-preconditions :equality)
(:types 	location locatable - object
	driver truck obj - locatable
)

(:predicates (at ?obj - locatable ?loc - location)
	(in ?obj1 - obj ?obj - truck)
	(driving ?d - driver ?v - truck)
	(link ?x - location ?y - location)
	(path ?x - location ?y - location)
	(empty ?v - truck)
)

(:functions (time-to-walk ?l1 - location ?l2 - location)
	(time-to-drive ?l1 - location ?l2 - location)
	(driven )
	(walked )
)

(:action load-truck
	:parameters (?a - obj ?b - truck ?c - location)
	:precondition (and (at ?a ?c)
	(at ?b ?c)
	(<= (* (driven ) -1) 0)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.5805) (* (walked ) 0.8143)) 175.3035)
	(<= (walked ) 144))
	:effect (and (in ?a ?b)
		(not (at ?a ?c)) 
		))

(:action unload-truck
	:parameters (?a - obj ?b - truck ?c - location)
	:precondition (and (at ?b ?c)
	(in ?a ?b)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.9618) (* (walked ) 0.2738)) 318.3477)
	(<= (+ (* (walked ) 0.0759) (* (driven ) -0.9971)) -12.9625)
	(<= (+ (* (walked ) 0.7809) (* (driven ) -0.6247)) 59.3460)
	(<= (+ (* (walked ) 0.9860) (* (driven ) -0.1669)) 93.5923))
	:effect (and (at ?a ?c)
		(not (in ?a ?b)) 
		))

(:action board-truck
	:parameters (?a - driver ?b - truck ?c - location)
	:precondition (and (at ?a ?c)
	(at ?b ?c)
	(empty ?b)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.3578) (* (walked ) 0.9338)) 186.7066)
	(<= (+ (* (driven ) 0.4651) (* (walked ) -0.8852)) 112.5610)
	(<= (+ (* (walked ) 0.5704) (* (driven ) -0.8214)) 0)
	(<= (walked ) 144))
	:effect (and (driving ?a ?b)
		(not (at ?a ?c))
		(not (empty ?b)) 
		))

(:action disembark-truck
	:parameters (?a - driver ?b - truck ?c - location)
	:precondition (and (at ?b ?c)
	(driving ?a ?b)
	(<= (+ (* (driven ) 0.3798) (* (walked ) -0.9251)) 91.9177)
	(<= (+ (* (walked ) -0.8764) (* (driven ) -0.4815)) -116.5349)
	(<= (+ (* (walked ) 0.8902) (* (driven ) -0.4555)) 61.6963)
	(<= (+ (* (walked ) 0.9259) (* (driven ) 0.3777)) 205.8568)
	(<= (walked ) 144))
	:effect (and (at ?a ?c)
		(empty ?b)
		(not (driving ?a ?b)) 
		))

(:action drive-truck
	:parameters (?a - truck ?b - location ?c - location ?d - driver)
	:precondition (and (at ?a ?b)
	(driving ?d ?a)
	(link ?b ?c)
	(link ?c ?b)
	(<= (* (driven ) -1) 0)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.0069) (+ (* (walked ) 0.0132) (* (time-to-drive ?c ?b) 0.9999))) 101.8935)
	(<= (+ (* (driven ) 0.0502) (+ (* (time-to-drive ?c ?b) -0.9980) (* (walked ) -0.0385))) -12.3212)
	(<= (+ (* (driven ) 0.0730) (+ (* (walked ) 0.0302) (* (time-to-drive ?c ?b) -0.9969))) -4.7611)
	(<= (+ (* (driven ) 0.2359) (+ (* (walked ) 0.7193) (* (time-to-drive ?c ?b) -0.6534))) 107.9757)
	(<= (+ (* (driven ) 0.2915) (+ (* (time-to-drive ?c ?b) 0.8368) (* (walked ) -0.4634))) 120.7464)
	(<= (+ (* (time-to-drive ?c ?b) 0.7200) (+ (* (walked ) -0.2000) (* (driven ) -0.6646))) 43.1975)
	(<= (+ (* (time-to-drive ?c ?b) 0.8224) (+ (* (driven ) 0.4931) (* (walked ) -0.2837))) 187.0617)
	(<= (+ (* (time-to-drive ?c ?b) 0.8302) (+ (* (driven ) 0.5117) (* (walked ) -0.2213))) 197.8870)
	(<= (+ (* (time-to-drive ?c ?b) 0.8912) (+ (* (driven ) 0.0990) (* (walked ) -0.4426))) 77.4372)
	(<= (+ (* (time-to-drive ?c ?b) 0.9841) (* (walked ) -0.1778)) 82.6610)
	(<= (+ (* (time-to-drive ?c ?b) 0.9858) (+ (* (walked ) -0.1289) (* (driven ) -0.1075))) 80.0130)
	(<= (+ (* (time-to-drive ?c ?b) 0.9999) (* (walked ) -0.0164)) 97.6262)
	(<= (+ (* (walked ) -0.0300) (* (time-to-drive ?c ?b) -0.9996)) -12.9942)
	(<= (+ (* (walked ) 0.0671) (+ (* (driven ) -0.4675) (* (time-to-drive ?c ?b) -0.8814))) -11.4585)
	(<= (+ (* (walked ) 0.4348) (+ (* (driven ) 0.1123) (* (time-to-drive ?c ?b) -0.8935))) 37.9197)
	(<= (+ (* (walked ) 0.6332) (* (time-to-drive ?c ?b) -0.7740)) 55.5842)
	(<= (+ (* (walked ) 0.7165) (+ (* (time-to-drive ?c ?b) -0.4657) (* (driven ) -0.5194))) 56.6003)
	(<= (+ (* (walked ) 0.8456) (+ (* (time-to-drive ?c ?b) -0.4698) (* (driven ) -0.2537))) 74.7855)
	(<= (walked ) 144)
	(= (time-to-drive ?b ?c) (time-to-drive ?c ?b)))
	:effect (and (at ?a ?c)
		(not (at ?a ?b))
(increase (driven ) (time-to-drive ?b ?c))))

(:action walk
	:parameters (?a - driver ?b - location ?c - location)
	:precondition (and (at ?a ?b)
	(path ?b ?c)
	(path ?c ?b)
	(<= (* (driven ) -1) 0)
	(<= (* (time-to-walk ?c ?b) -1) -8)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) -0.0057) (+ (* (walked ) -0.1758) (* (time-to-walk ?c ?b) -0.9844))) -22.6415)
	(<= (+ (* (driven ) 0.0090) (+ (* (walked ) -0.0225) (* (time-to-walk ?c ?b) -0.9997))) -9.8847)
	(<= (+ (* (driven ) 0.0107) (+ (* (walked ) -0.0086) (* (time-to-walk ?c ?b) -0.9999))) -8.5731)
	(<= (+ (* (driven ) 0.0596) (+ (* (time-to-walk ?c ?b) -0.7947) (* (walked ) -0.6040))) -18.2792)
	(<= (+ (* (driven ) 0.0781) (+ (* (walked ) 0.4226) (* (time-to-walk ?c ?b) 0.9029))) 84.8747)
	(<= (+ (* (driven ) 0.1109) (+ (* (walked ) 0.4053) (* (time-to-walk ?c ?b) 0.9074))) 85.2979)
	(<= (+ (* (walked ) 0.5955) (+ (* (driven ) 0.1776) (* (time-to-walk ?c ?b) 0.7835))) 103.1094)
	(<= (+ (* (walked ) 0.6190) (+ (* (time-to-walk ?c ?b) 0.7646) (* (driven ) 0.1796))) 104.7871)
	(<= (+ (* (walked ) 0.6287) (+ (* (driven ) 0.0407) (* (time-to-walk ?c ?b) 0.7766))) 97.9258)
	(<= (+ (* (walked ) 0.7087) (+ (* (driven ) -0.6851) (* (time-to-walk ?c ?b) -0.1687))) 58.1792)
	(= (time-to-walk ?b ?c) (time-to-walk ?c ?b)))
	:effect (and (at ?a ?c)
		(not (at ?a ?b))
(increase (walked ) (time-to-walk ?b ?c))))

)