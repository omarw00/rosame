	(<= (+ (* (walked ) 0.7906) (* (driven ) -0.6124)) 118.5597)
	(<= (walked ) 356))
	:effect (and (at ?a ?c)
		(not (in ?a ?b)) 
		))

(:action board-truck
	:parameters (?a - driver ?b - truck ?c - location)
	:precondition (and (at ?a ?c)
	(at ?b ?c)
	(empty ?b)
	(<= (* (driven ) -1) 0)
