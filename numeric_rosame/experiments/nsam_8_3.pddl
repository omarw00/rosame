	(<= (+ (* (walked ) 0.7879) (+ (* (time-to-drive ?loc-to ?loc-from) -0.4525) (* (driven ) -0.4177))) 197.5224)
	(<= (+ (* (walked ) 0.8729) (+ (* (time-to-drive ?loc-to ?loc-from) 0.0168) (* (driven ) -0.4876))) 220.3033)
	(<= (walked ) 370)
	(= (time-to-drive ?loc-from ?loc-to) (time-to-drive ?loc-to ?loc-from)))
	:effect (and (at ?truck ?loc-to)
		(not (at ?truck ?loc-from))
(increase (driven ) (time-to-drive ?loc-from ?loc-to))))

