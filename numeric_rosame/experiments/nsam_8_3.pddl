(define (domain driverlog)
(:requirements :typing :fluents :negative-preconditions :equality)
(:types 	location locatable - object
	driver truck obj - locatable
)

(:predicates (at ?obj - locatable ?loc - location)
	(in ?obj1 - obj ?obj - truck)
	(driving ?d - driver ?v - truck)
	(link ?x - location ?y - location)
	(path ?x - location ?y - location)
	(empty ?v - truck)
)

(:functions (time-to-walk ?l1 - location ?l2 - location)
	(time-to-drive ?l1 - location ?l2 - location)
	(driven )
	(walked )
)

(:action load-truck
	:parameters (?obj - obj ?truck - truck ?loc - location)
	:precondition (and (at ?obj ?loc)
	(at ?truck ?loc)
	(not (in ?obj ?truck))
	(<= (* (driven ) -1) 0)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.9977) (* (walked ) -0.0674)) 185.5769)
	(<= (+ (* (walked ) 0.0229) (* (driven ) -0.9997)) 2.8161)
	(<= (+ (* (walked ) 0.8734) (* (driven ) -0.4871)) 220.3732))
	:effect (and (in ?obj ?truck)
		(not (at ?obj ?loc)) 
		))

(:action unload-truck
	:parameters (?obj - obj ?truck - truck ?loc - location)
	:precondition (and (at ?truck ?loc)
	(in ?obj ?truck)
	(not (at ?obj ?loc))
	(<= (* (walked ) -1) 0)
	(<= (+ (* (walked ) -0.2076) (* (driven ) -0.9782)) -58.6093)
	(<= (+ (* (walked ) -0.5682) (* (driven ) -0.8229)) -65.8318)
	(<= (+ (* (walked ) 0.3692) (* (driven ) 0.9293)) 345.7146)
	(<= (+ (* (walked ) 0.8828) (* (driven ) -0.4697)) 221.4130)
	(<= (walked ) 370))
	:effect (and (at ?obj ?loc)
		(not (in ?obj ?truck)) 
		))

(:action board-truck
	:parameters (?driver - driver ?truck - truck ?loc - location)
	:precondition (and (at ?driver ?loc)
	(at ?truck ?loc)
	(empty ?truck)
	(not (driving ?driver ?truck))
	(<= (* (driven ) -1) 0)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.4044) (* (walked ) -0.9146)) 61.0570)
	(<= (+ (* (driven ) 0.9473) (* (walked ) 0.3203)) 246.4936)
	(<= (+ (* (walked ) 0.4197) (* (driven ) -0.9077)) 36.9316)
	(<= (+ (* (walked ) 0.6054) (* (driven ) -0.7959)) 75.3458))
	:effect (and (driving ?driver ?truck)
		(not (at ?driver ?loc))
		(not (empty ?truck)) 
		))

(:action disembark-truck
	:parameters (?driver - driver ?truck - truck ?loc - location)
	:precondition (and (at ?truck ?loc)
	(driving ?driver ?truck)
	(not (at ?driver ?loc))
	(not (empty ?truck))
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.8905) (* (walked ) 0.4549)) 412.3214)
	(<= (+ (* (walked ) -0.5124) (* (driven ) -0.8587)) -129.6707)
	(<= (+ (* (walked ) 0.7740) (* (driven ) -0.6332)) 112.8570))
	:effect (and (at ?driver ?loc)
		(empty ?truck)
		(not (driving ?driver ?truck)) 
		))

