	(<= (+ (* (time-to-drive ?loc-to ?loc-from) 0.9998) (* (walked ) 0.0197)) 94.9816)
	(<= (+ (* (walked ) 0.0018) (+ (* (time-to-drive ?loc-to ?loc-from) -0.9999) (* (driven ) -0.0100))) -2.5679)
	(<= (+ (* (walked ) 0.0050) (+ (* (time-to-drive ?loc-to ?loc-from) 0.9990) (* (driven ) -0.0439))) 90.9111)
	(<= (+ (* (walked ) 0.0117) (+ (* (time-to-drive ?loc-to ?loc-from) -0.1179) (* (driven ) -0.9930))) -0.3538)
