(:action load-truck
	:parameters (?obj - obj ?truck - truck ?loc - location)
	:precondition (and (at ?obj ?loc)
	(at ?truck ?loc)
	(not (in ?obj ?truck))
	(<= (* (driven ) -1) 0)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (walked ) 0.0189) (* (driven ) -0.9998)) 1.7921)
	(<= (+ (* (walked ) 0.8045) (* (driven ) 0.5940)) 261.3584)
	(<= (walked ) 254))
