	(<= (+ (* (time-to-drive ?c ?b) 0.9734) (+ (* (driven ) 0.2281) (* (walked ) -0.0207))) 103.5616)
	(<= (+ (* (time-to-drive ?c ?b) 0.9961) (+ (* (driven ) -0.0796) (* (walked ) -0.0389))) 70.0049)
	(<= (+ (* (time-to-drive ?c ?b) 0.9979) (* (walked ) -0.0649)) 70.8503)
	(<= (+ (* (walked ) 0.0093) (+ (* (driven ) -0.0510) (* (time-to-drive ?c ?b) -0.9987))) -8.9879)
