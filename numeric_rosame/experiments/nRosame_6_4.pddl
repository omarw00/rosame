(define (domain driverlog)
(:requirements :typing :fluents :negative-preconditions :equality)
(:types 	location locatable - object
	driver truck obj - locatable
)

(:predicates (at ?obj - locatable ?loc - location)
	(in ?obj1 - obj ?obj - truck)
	(driving ?d - driver ?v - truck)
	(link ?x - location ?y - location)
	(path ?x - location ?y - location)
	(empty ?v - truck)
)

(:functions (time-to-walk ?l1 - location ?l2 - location)
	(time-to-drive ?l1 - location ?l2 - location)
	(driven )
	(walked )
)

(:action load-truck
	:parameters (?a - obj ?b - truck ?c - location)
	:precondition (and (at ?a ?c)
	(at ?b ?c)
	(<= (* (driven ) -1) 0)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.2971) (* (walked ) -0.9549)) 2.6736)
	(<= (+ (* (driven ) 0.9712) (* (walked ) -0.2382)) 129.8491)
	(<= (+ (* (driven ) 0.9813) (* (walked ) -0.1927)) 135.7530)
	(<= (+ (* (walked ) 0.3404) (* (driven ) -0.9403)) 41.8743)
	(<= (+ (* (walked ) 0.7090) (* (driven ) -0.7052)) 113.5136))
	:effect (and (in ?a ?b)
		(not (at ?a ?c)) 
		))

(:action unload-truck
	:parameters (?a - obj ?b - truck ?c - location)
	:precondition (and (at ?b ?c)
	(in ?a ?b)
	(<= (+ (* (driven ) -0.9983) (* (walked ) -0.0575)) -53.3290)
	(<= (+ (* (driven ) 0.1961) (* (walked ) -0.9806)) 15.6893)
	(<= (+ (* (walked ) -0.5682) (* (driven ) -0.8229)) -65.8318)
	(<= (+ (* (walked ) 0.1944) (* (driven ) 0.9809)) 292.6324)
	(<= (+ (* (walked ) 0.6917) (* (driven ) -0.7222)) 94.1340)
	(<= (walked ) 370))
	:effect (and (at ?a ?c)
		(not (in ?a ?b)) 
		))

(:action board-truck
	:parameters (?a - driver ?b - truck ?c - location)
	:precondition (and (at ?a ?c)
	(at ?b ?c)
	(empty ?b)
	(<= (* (driven ) -1) 0)
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.4044) (* (walked ) -0.9146)) 61.0570)
	(<= (+ (* (driven ) 0.9473) (* (walked ) 0.3203)) 246.4936)
	(<= (+ (* (walked ) 0.4197) (* (driven ) -0.9077)) 36.9316)
	(<= (+ (* (walked ) 0.6054) (* (driven ) -0.7959)) 75.3458))
	:effect (and (driving ?a ?b)
		(not (at ?a ?c))
		(not (empty ?b)) 
		))

(:action disembark-truck
	:parameters (?a - driver ?b - truck ?c - location)
	:precondition (and (at ?b ?c)
	(driving ?a ?b)
	(<= (+ (* (driven ) 0.2892) (* (walked ) -0.9573)) 43.6757)
	(<= (+ (* (walked ) -0.5124) (* (driven ) -0.8587)) -129.6707)
	(<= (+ (* (walked ) 0.0487) (* (driven ) 0.9988)) 291.7019)
	(<= (+ (* (walked ) 0.7740) (* (driven ) -0.6332)) 112.8570))
	:effect (and (at ?a ?c)
		(empty ?b)
		(not (driving ?a ?b)) 
		))

