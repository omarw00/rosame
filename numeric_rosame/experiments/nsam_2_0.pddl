(define (domain driverlog)
(:requirements :typing :fluents :negative-preconditions :equality)
(:types 	location locatable - object
	driver truck obj - locatable
)

(:predicates (at ?obj - locatable ?loc - location)
	(in ?obj1 - obj ?obj - truck)
	(driving ?d - driver ?v - truck)
	(link ?x - location ?y - location)
	(path ?x - location ?y - location)
	(empty ?v - truck)
)

(:functions (time-to-walk ?l1 - location ?l2 - location)
	(time-to-drive ?l1 - location ?l2 - location)
	(driven )
	(walked )
)

(:action load-truck
	:parameters (?obj - obj ?truck - truck ?loc - location)
	:precondition (and (at ?obj ?loc)
	(at ?truck ?loc)
	(not (empty ?truck))
	(not (in ?obj ?truck))
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.9970) (* (walked ) -0.0775)) 343.9612)
	(<= (+ (* (walked ) -0.2873) (* (driven ) -0.9578)) -51.7226)
	(<= (walked ) 180))
	:effect (and (in ?obj ?truck)
		(not (at ?obj ?loc)) 
		))

(:action unload-truck
	:parameters (?obj - obj ?truck - truck ?loc - location)
	:precondition (and (at ?truck ?loc)
	(in ?obj ?truck)
	(not (at ?obj ?loc))
	(not (empty ?truck))
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) -1) (* (walked ) -0.0056)) -133.9979)
	(<= (+ (* (driven ) 0.9593) (* (walked ) -0.2825)) 382.7530)
	(<= (walked ) 180))
	:effect (and (at ?obj ?loc)
		(not (in ?obj ?truck)) 
		))

(:action board-truck
	:parameters (?driver - driver ?truck - truck ?loc - location)
	:precondition (and (at ?driver ?loc)
	(at ?truck ?loc)
	(empty ?truck)
	(not (driving ?driver ?truck))
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.8836) (* (walked ) -0.4683)) 423.2314)
	(<= (+ (* (walked ) 0.7822) (* (driven ) -0.6230)) 0)
	(<= (+ (* (walked ) 0.9601) (* (driven ) -0.2796)) 109.6197))
	:effect (and (driving ?driver ?truck)
		(not (at ?driver ?loc))
		(not (empty ?truck)) 
		))

(:action disembark-truck
	:parameters (?driver - driver ?truck - truck ?loc - location)
	:precondition (and (at ?truck ?loc)
	(driving ?driver ?truck)
	(not (at ?driver ?loc))
	(not (empty ?truck))
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.9295) (* (walked ) -0.3687)) 519.6136)
	(<= (+ (* (walked ) -0.8148) (* (driven ) -0.5797)) -277.6830)
	(<= (+ (* (walked ) 0.9601) (* (driven ) -0.2796)) 109.6197)
	(<= (walked ) 300))
	:effect (and (at ?driver ?loc)
		(empty ?truck)
		(not (driving ?driver ?truck)) 
		))

(:action drive-truck
	:parameters (?truck - truck ?loc-from - location ?loc-to - location ?driver - driver)
	:precondition (and (at ?truck ?loc-from)
	(driving ?driver ?truck)
	(link ?loc-from ?loc-to)
	(link ?loc-to ?loc-from)
	(not (at ?driver ?loc-from))
	(not (at ?driver ?loc-to))
	(not (at ?truck ?loc-to))
	(not (empty ?truck))
	(not (path ?loc-from ?loc-to))
	(not (path ?loc-to ?loc-from))
	(<= (* (walked ) -1) 0)
	(<= (+ (* (driven ) 0.0383) (+ (* (time-to-drive ?loc-to ?loc-from) 0.9979) (* (walked ) -0.0517))) 98.2002)
	(<= (+ (* (driven ) 0.3660) (+ (* (time-to-drive ?loc-to ?loc-from) -0.8772) (* (walked ) -0.3109))) 105.1250)
	(<= (+ (* (driven ) 0.4899) (+ (* (time-to-drive ?loc-to ?loc-from) 0.8596) (* (walked ) -0.1450))) 303.4316)
	(<= (+ (* (time-to-drive ?loc-to ?loc-from) 0.8843) (+ (* (walked ) -0.1916) (* (driven ) -0.4258))) 47.7527)
	(<= (+ (* (time-to-drive ?loc-to ?loc-from) 0.9974) (* (walked ) -0.0720)) 79.7922)
	(<= (+ (* (time-to-drive ?loc-to ?loc-from) 0.9982) (+ (* (driven ) 0.0290) (* (walked ) -0.0528))) 93.7219)
	(<= (+ (* (walked ) -0.0127) (+ (* (time-to-drive ?loc-to ?loc-from) -0.9921) (* (driven ) -0.1249))) -53.5731)
	(<= (+ (* (walked ) 0.0764) (* (time-to-drive ?loc-to ?loc-from) -0.9971)) -16.9503)
	(<= (+ (* (walked ) 0.1020) (+ (* (driven ) -0.0225) (* (time-to-drive ?loc-to ?loc-from) -0.9945))) -23.5099)
	(<= (+ (* (walked ) 0.1850) (+ (* (time-to-drive ?loc-to ?loc-from) -0.8538) (* (driven ) -0.4866))) -46.1063)
	(<= (+ (* (walked ) 0.9137) (+ (* (time-to-drive ?loc-to ?loc-from) -0.3530) (* (driven ) -0.2012))) 131.6392)
	(<= (time-to-drive ?loc-to ?loc-from) 93)
	(= (time-to-drive ?loc-from ?loc-to) (time-to-drive ?loc-to ?loc-from)))
	:effect (and (at ?truck ?loc-to)
		(not (at ?truck ?loc-from))
(increase (driven ) (time-to-drive ?loc-from ?loc-to))))

(:action walk
	:parameters (?driver - driver ?loc-from - location ?loc-to - location)
	:precondition (and (at ?driver ?loc-from)
	(not (at ?driver ?loc-to))
	(not (link ?loc-from ?loc-to))
	(not (link ?loc-to ?loc-from))
	(path ?loc-from ?loc-to)
	(path ?loc-to ?loc-from)
	(<= (+ (* (driven ) -0.0731) (+ (* (time-to-walk ?loc-to ?loc-from) -0.9964) (* (walked ) -0.0433))) -91.6671)
	(<= (+ (* (driven ) 0.0260) (+ (* (time-to-walk ?loc-to ?loc-from) 0.9986) (* (walked ) -0.0454))) 91.8741)
	(<= (+ (* (driven ) 0.1381) (+ (* (walked ) 0.0450) (* (time-to-walk ?loc-to ?loc-from) -0.9894))) 20.0578)
	(<= (+ (* (time-to-walk ?loc-to ?loc-from) 0.4302) (+ (* (driven ) 0.3674) (* (walked ) -0.8246))) 39.5799)
	(<= (+ (* (walked ) 0.0434) (+ (* (time-to-walk ?loc-to ?loc-from) 0.9987) (* (driven ) -0.0249))) 91.8848)
	(<= (+ (* (walked ) 0.3376) (+ (* (driven ) -0.1419) (* (time-to-walk ?loc-to ?loc-from) -0.9305))) -50.8275)
	(= (time-to-walk ?loc-from ?loc-to) (time-to-walk ?loc-to ?loc-from)))
	:effect (and (at ?driver ?loc-to)
		(not (at ?driver ?loc-from))
(increase (walked ) (time-to-walk ?loc-from ?loc-to))))

)