	(<= (+ (* (y ?a) 0.7650) (+ (* (x ?a) 0.0573) (* (d ?b) -0.6415))) 14.0483)
	(<= (+ (* (y ?a) 0.7738) (+ (* (x ?a) 0.0836) (* (d ?b) -0.6279))) 15.1571))
	:effect (and (saved ?b) 
		))

)