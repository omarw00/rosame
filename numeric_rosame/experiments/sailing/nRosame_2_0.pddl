(define (domain sailing)
(:requirements :typing :fluents :negative-preconditions :equality)
(:types 	boat person - object
)

(:predicates (saved ?t - person)
)

(:functions (x ?b - boat)
	(y ?b - boat)
	(d ?t - person)
)

(:action go_north_east
	:parameters (?a - boat)
	:precondition (and (<= (* (y ?a) -1) 0)
	(<= (+ (* (x ?a) 0.7071) (* (y ?a) -0.7071)) 0)
	(<= (+ (* (x ?a) 0.7415) (* (y ?a) -0.6709)) 0.4237)
	(<= (+ (* (x ?a) 0.9701) (* (y ?a) 0.2425)) 30.8020)
	(<= (+ (* (y ?a) -0.4668) (* (x ?a) -0.8844)) 1.7688)
	(<= (+ (* (y ?a) 0.2425) (* (x ?a) -0.9701)) 29.1043)
	(<= (+ (* (y ?a) 0.6601) (* (x ?a) -0.7512)) 50.8743)
	(<= (+ (* (y ?a) 0.7071) (* (x ?a) -0.7071)) 54.4472))
	:effect (and 
(increase (x ?a) 1.5000)		
(increase (y ?a) 1.5000)))

(:action go_north_west
	:parameters (?a - boat)
	:precondition (and (<= (+ (* (x ?a) -0.7071) (* (y ?a) -0.7071)) -10.6066)
	(<= (+ (* (x ?a) 0.7071) (* (y ?a) 0.7071)) 70.7107)
	(<= (+ (* (x ?a) 0.7415) (* (y ?a) -0.6709)) 0.5297)
	(<= (+ (* (x ?a) 0.9701) (* (y ?a) 0.2425)) 32.6210)
	(<= (+ (* (y ?a) 0.2425) (* (x ?a) -0.9701)) 27.2853)
	(<= (+ (* (y ?a) 0.3796) (* (x ?a) -0.9252)) 34.6938))
	:effect (and 
(increase (y ?a) 1.5000)		
(decrease (x ?a) 1.5000)))

(:action save_person
	:parameters (?a - boat ?b - person)
	:precondition (and (<= (+ (* (d ?b) 0.3841) (+ (* (y ?a) -0.5121) (* (x ?a) -0.7682))) -2.6888)
	(<= (+ (* (d ?b) 0.5223) (+ (* (x ?a) 0.6908) (* (y ?a) -0.4999))) 2.3927)
	(<= (+ (* (d ?b) 0.5520) (+ (* (x ?a) 0.5919) (* (y ?a) -0.5874))) -1.6359)
	(<= (+ (* (d ?b) 0.5722) (+ (* (x ?a) -0.5626) (* (y ?a) -0.5967))) -1.5120)
	(<= (+ (* (d ?b) 0.5750) (+ (* (y ?a) -0.5337) (* (x ?a) -0.6201))) 2.8976)
	(<= (+ (* (d ?b) 0.5824) (+ (* (y ?a) -0.5563) (* (x ?a) -0.5927))) 1.6111)
	(<= (+ (* (d ?b) 0.6766) (+ (* (y ?a) -0.6540) (* (x ?a) -0.3383))) 1.2179)
	(<= (+ (* (x ?a) 0.0046) (+ (* (y ?a) 0.7237) (* (d ?b) -0.6901))) 10.1373)
	(<= (+ (* (x ?a) 0.5079) (+ (* (d ?b) 0.6021) (* (y ?a) -0.6161))) -1.4136)
	(<= (+ (* (x ?a) 0.5096) (+ (* (d ?b) 0.6022) (* (y ?a) -0.6146))) -1.3156)
	(<= (+ (* (y ?a) 0.5450) (+ (* (x ?a) -0.0399) (* (d ?b) -0.8375))) -2.7118)
