(define (domain sailing)
(:requirements :typing :fluents :negative-preconditions :equality)
(:types 	boat person - object
)

(:predicates (saved ?t - person)
)

(:functions (x ?b - boat)
	(y ?b - boat)
	(d ?t - person)
)

(:action go_north_east
	:parameters (?a - boat)
	:precondition (and (<= (+ (* (x ?a) 0.7071) (* (y ?a) -0.7071)) 0.7071)
	(<= (+ (* (y ?a) -0.0370) (* (x ?a) -0.9993)) -0.9993)
	(<= (+ (* (y ?a) 0.0345) (* (x ?a) 0.9994)) 18.0582)
	(<= (+ (* (y ?a) 0.4679) (* (x ?a) 0.8838)) 42.2139)
	(<= (+ (* (y ?a) 0.7071) (* (x ?a) -0.7071)) 58.6899))
	:effect (and 
(increase (y ?a) 1.5000)		
(increase (x ?a) 1.5000)))

(:action go_north_west
	:parameters (?a - boat)
	:precondition (and (<= (+ (* (x ?a) -0.7071) (* (y ?a) -0.7071)) -26.1630)
	(<= (+ (* (x ?a) 0.7071) (* (y ?a) 0.7071)) 64.3467)
