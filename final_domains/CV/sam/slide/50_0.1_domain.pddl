(define (domain gripper-strips-4ops)
(:requirements :adl :equality :negative-preconditions)
(:types 	tile position - object
)

(:predicates (at ?a - tile ?b - position ?c - position)
	(blank ?a - position ?b - position)
	(inc ?a - position ?b - position)
	(dec ?a - position ?b - position)
)

(:action move-up
	:parameters (?a - position ?b - position ?c - position ?d - tile)
	:precondition (and (blank ?a ?c)(not (= ?a ?c))
	(not (= ?a ?b))
	(not (= ?b ?c)))
	:effect (and (at ?d ?a ?c)
		(at ?d ?b ?a)
		(blank ?a ?b)
		(blank ?b ?a)
		(blank ?c ?b)
		(dec ?a ?b)
		(dec ?b ?a)
		(dec ?b ?c)
		(inc ?a ?b)
		(inc ?b ?a)
		(inc ?c ?b)
		(not (blank ?a ?c)) 
		))

(:action move-down
	:parameters (?a - position ?b - position ?c - position ?d - tile)
	:precondition (and (at ?d ?a ?b)
	(at ?d ?c ?a)
	(blank ?a ?c)
	(blank ?b ?c)
	(blank ?c ?a)
	(dec ?a ?c)
	(dec ?c ?a)
	(dec ?c ?b)
	(inc ?a ?c)
	(inc ?b ?c)
	(inc ?c ?a)(not (= ?a ?c))
	(not (= ?a ?b))
	(not (= ?b ?c)))
	:effect (and (not (at ?d ?a ?b))
		(not (at ?d ?c ?a))
		(not (blank ?a ?c))
		(not (blank ?b ?c))
		(not (blank ?c ?a))
		(not (dec ?a ?c))
		(not (dec ?c ?a))
		(not (dec ?c ?b))
		(not (inc ?a ?c))
		(not (inc ?b ?c))
		(not (inc ?c ?a)) 
		))

(:action move-left
	:parameters (?a - position ?b - position ?c - position ?d - tile)
	:precondition (and (blank ?b ?c)
	(blank ?c ?b)
	(dec ?b ?c)
	(dec ?c ?b)
	(inc ?c ?b)(not (= ?a ?c))
	(not (= ?a ?b))
	(not (= ?b ?c)))
	:effect (and (blank ?b ?a)
		(dec ?a ?b)
		(dec ?b ?a)
		(inc ?a ?b)
		(inc ?b ?a)
		(not (blank ?b ?c))
		(not (blank ?c ?b))
		(not (dec ?b ?c))
		(not (dec ?c ?b))
		(not (inc ?c ?b)) 
		))

(:action move-right
	:parameters (?a - position ?b - position ?c - position ?d - tile)
	:precondition (and (dec ?b ?c)
	(dec ?c ?b)(not (= ?a ?c))
	(not (= ?a ?b))
	(not (= ?b ?c)))
	:effect (and (not (dec ?b ?c))
		(not (dec ?c ?b)) 
		))

)